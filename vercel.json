{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "cleanUrls": true,
        "rewrites": [
          {
            "source": "/(.*)",
            "destination": "/index.html"
          }
        ],
        "trailingSlash": false,
        "headers": [
          {
            "source": "/tmp/(.*)",
            "headers": [
              {
                "key": "Cache-Control",
                "value": "public, max-age=31536000, immutable"
              }
            ]
          }
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {},
  "public": true
  
}